{
  "debug.javascript.usePreview": false,
  "breadcrumbs.enabled": false,
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "editor.renderControlCharacters": false,
  "editor.fontFamily": "CaskaydiaCove Nerd Font Mono, Fira Code",
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.lineNumbers": "relative",
  "editor.renderWhitespace": "all",
  "editor.rulers": [80, 120],
  "editor.scrollBeyondLastLine": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoGuessEncoding": true,
  "workbench.list.automaticKeyboardNavigation": false,
  "git.ignoreMissingGitWarning": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.fontFamily": "Meslo LG S for Powerline, Fira Code",
  "terminal.integrated.rendererType": "dom",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.env.windows": {},
  "workbench.iconTheme": "Monokai Pro (Filter Ristretto) Icons",
  "workbench.activityBar.visible": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorTheme": "Atom One Dark",
  "workbench.sideBar.location": "left",
  "eslint.alwaysShowStatus": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "editor.fontWeight": "400",
  "editor.hideCursorInOverviewRuler": true,
  "editor.suggestSelection": "first",
  "editor.formatOnSave": true,
  "workbench.editor.showTabs": true,
  "tabnine.experimentalAutoImports": true,
  // whichkey config
  "whichkey.sortOrder": "alphabetically",
  "whichkey.delay": 0,
  "whichkey.bindings": [
    {
      "key": "f",
      "name": "advanced search",
      "type": "command",
      "command": "workbench.view.search"
    },
    {
      "key": "e",
      "name": "Open Explorer",
      "type": "command",
      "command": "workbench.view.explorer"
    },
    {
      "key": ";",
      "name": "comment",
      "type": "command",
      "command": "vscode-neovim.send",
      "args": "<C-/>"
    },
    {
      "key": ".",
      "name": "quick fix options",
      "type": "command",
      "command": "editor.action.quickFix"
    },
    {
      "key": "p",
      "name": "quick open",
      "type": "command",
      "command": "workbench.action.quickOpen"
    },
    {
      "key": "h",
      "name": "Jump left",
      "type": "command",
      "command": "workbench.action.focusLeftGroup"
    },
    {
      "key": "l",
      "name": "Jump right",
      "type": "command",
      "command": "workbench.action.focusRightGroup"
    },
    {
      "key": "w",
      "name": "Window",
      "type": "bindings",
      "bindings": [
        {
          "key": "v",
          "name": "Split vertical",
          "type": "command",
          "command": "workbench.action.splitEditor"
        }
      ]
    }
  ],
  "files.exclude": {
    "**/.tscn": true
  },
  "window.menuBarVisibility": "classic",
  "window.titleBarStyle": "custom",
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "vscode-neovim.logOutputToConsole": true
  // "vscode-neovim.neovimInitVimPaths.linux": "/home/Gothivisore/.config/nvim/init.vim",
}
