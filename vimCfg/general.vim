" files import
source ~/.config/nvim/autoload/plug.vim

 "Theme
colorscheme gruvbox
set background=dark

" editor config
highlight clear SignColumn
highlight clear LineNr

set number relativenumber
set t_Co=256
set nobackup
set clipboard=unnamedplus
set nowritebackup
set cursorline
set smartindent
set autoindent
set smarttab
set splitbelow
set splitright
set scrolloff=8
set hidden
