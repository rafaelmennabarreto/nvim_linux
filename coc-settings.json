{
  "coc.preferences.formatOnSaveFiletypes": ["css", "json", "ts", "tsx"],
  "explorer.width": 30,
  "explorer.buffer.root.template": "[icon & 1] OPEN EDITORS",
  "explorer.file.root.template": "[icon & 1] PROJECT ([root])",
  "explorer.file.showHiddenFiles": true,
  "explorer.file.autoReveal": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "sv": ["wait", "open:vsplit"],
    "rn": "rename",
    "cp": "copyFile",
    "rm": "delete",
  },
 "godot.enable": true, // states wether this extension should be enabled
 "godot.lsp_server_port" : 6008
}
